{
  "manifest_version": 3,
  "name": "Download Any Canvas",
  "version": "1.1",
  "description": "Download merged images from any canvas elements on any site",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_title": "Download Canvas",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
